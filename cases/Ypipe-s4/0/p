FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include        "include/initialConditions"

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    //- Set patchGroups for constraint patches
    //#includeEtc "caseDicts/setConstraintTypes"

    Inlet.stl
    {
        type            zeroGradient;
    }

    Outlet.stl
    {
        type            fixedValue;
        value           uniform 0;
    }

    Inlet2.stl
    {
        type            zeroGradient;
    }

    Walls.stl
    {
        type            zeroGradient;
    }

}

